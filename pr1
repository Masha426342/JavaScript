const readline = require ("readline").createInterface({ //создаем интерфейс
    input: process.stdin,
    output: process.stdout
});
readline.question("Введите число N: ", (N) => { // просим пользователя ввести последнее число массива
N = parseInt(N); // Преобразуем введенное значение в число

let arr = []; //создаем список

for (let i = 1; i <= N; i++) { 
    arr.push(i)// наполняем список до числа N
}

console.log("Готовый массив: ", arr);// выводим результат
})
