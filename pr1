const prompt = require("prompt-sync")(); // импорт и инициализации библиотеки prompt-sync, для упрощения кода интерфейса
let N = +prompt("Введите число N: ");// просим пользователя ввести последнее число массива
let arr = []; //создаем список

for (let i = 1; i <= N; i++) { 
    arr.push(i)// наполняем список до числа N
}

console.log("Готовый массив: ", arr);// выводим результат